{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "moduleResolution": "node",
        "noImplicitAny": false,
        "sourceMap": false,
        "outDir": "built",
        "experimentalDecorators": true,
        "declaration": true,
        "noLib": false,
        "preserveConstEnums": true
    },
    "exclude": [
        "node_modules", "built"
    ]
}